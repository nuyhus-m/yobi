<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple_sub"
    android:paddingVertical="@dimen/space_24dp">

    <include
        android:id="@+id/include_1"
        layout="@layout/layout_result_item"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/include_3"
        app:layout_constraintEnd_toStartOf="@id/include_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        app:layout_constraintWidth_percent="0.35" />

    <include
        android:id="@+id/include_2"
        layout="@layout/layout_result_item"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/include_1"
        app:layout_constraintTop_toTopOf="@id/include_1"
        app:layout_constraintWidth_percent="0.35" />

    <include
        android:id="@+id/include_3"
        layout="@layout/layout_result_item"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/space_24dp"
        app:layout_constraintBottom_toTopOf="@+id/include_5"
        app:layout_constraintStart_toStartOf="@id/include_1"
        app:layout_constraintTop_toBottomOf="@+id/include_1"
        app:layout_constraintWidth_percent="0.35" />

    <include
        android:id="@+id/include_4"
        layout="@layout/layout_result_item"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/include_2"
        app:layout_constraintTop_toTopOf="@id/include_3"
        app:layout_constraintWidth_percent="0.35" />

    <include
        android:id="@+id/include_5"
        layout="@layout/layout_result_item"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/space_24dp"
        app:layout_constraintBottom_toTopOf="@+id/include_7"
        app:layout_constraintStart_toStartOf="@id/include_1"
        app:layout_constraintTop_toBottomOf="@+id/include_3"
        app:layout_constraintWidth_percent="0.35" />

    <include
        android:id="@+id/include_6"
        layout="@layout/layout_result_item"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/include_2"
        app:layout_constraintTop_toTopOf="@id/include_5"
        app:layout_constraintWidth_percent="0.35" />

    <include
        android:id="@+id/include_7"
        layout="@layout/layout_result_item"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/space_24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/include_1"
        app:layout_constraintTop_toBottomOf="@+id/include_5"
        app:layout_constraintWidth_percent="0.35" />

    <include
        android:id="@+id/include_8"
        layout="@layout/layout_result_item"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/include_2"
        app:layout_constraintTop_toTopOf="@id/include_7"
        app:layout_constraintWidth_percent="0.35" />

</androidx.constraintlayout.widget.ConstraintLayout>